<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<portlet-definition version="5.0" xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition https://source.jasig.org/schemas/uportal/io/portlet-definition/portlet-definition-5.0.xsd" xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition" xmlns:ns6="https://source.jasig.org/schemas/uportal/io/subscribed-fragment" xmlns:ns5="https://source.jasig.org/schemas/uportal/io/stylesheet-descriptor" xmlns:ns8="https://source.jasig.org/schemas/uportal/io/user" xmlns:ns7="https://source.jasig.org/schemas/uportal/io/portlet-type" xmlns:ns2="https://source.jasig.org/schemas/uportal" xmlns:ns4="https://source.jasig.org/schemas/uportal/io/permission-owner" xmlns:ns3="https://source.jasig.org/schemas/uportal/io/event-aggregation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <title>Safety Information</title>
    <name>Safety Information</name>
    <fname>safety-information</fname>
    <desc>Safety Information</desc>
    <type>App Launcher</type>
    <timeout>5000</timeout>
    <portlet-descriptor>
        <ns2:webAppName>/jasig-widget-portlets</ns2:webAppName>
        <ns2:portletName>app-launcher</ns2:portletName>
    </portlet-descriptor>
     <category>Administration</category>
    <group>Everyone</group>
    <permissions>
        <permission system="UP_PORTLET_SUBSCRIBE" activity="BROWSE">
            <group>Everyone</group>
        </permission>
    </permissions>
    <parameter>
        <name>alternativeMaximizedLink</name>
        <value>https://www.google.com/</value>
    </parameter>
    <parameter>
        <name>blockImpersonation</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>chromeStyle</name>
        <value>default</value>
    </parameter>
    <parameter>
        <name>configurable</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>disableDynamicTitle</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>disablePortletEvents</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>editable</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasAbout</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasHelp</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hideFromMobile</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>iconUrl</name>
        <value>/uPortal/media/skins/respondr/cuesta/img/safety-info.png</value>
    </parameter>
    <parameter>
        <name>mobileIconUrl</name>
        <value>/uPortal/media/skins/respondr/cuesta/img/safety-info.png</value>
    </parameter>
    <portlet-preference>
        <name>AppDefinition.appUrl</name>
        <readOnly>false</readOnly>
        <value>https://www.google.com/</value>
    </portlet-preference>
    <portlet-preference>
        <name>AppDefinition.iconUrl</name>
        <readOnly>false</readOnly>
        <value>/uPortal/media/skins/respondr/cuesta/img/safety-info.png</value>
    </portlet-preference>
    <portlet-preference>
        <name>AppDefinition.redirect</name>
        <readOnly>false</readOnly>
        <value>false</value>
    </portlet-preference>
    <portlet-preference>
        <name>AppDefinition.title</name>
        <readOnly>false</readOnly>
        <value>Safety Information</value>
    </portlet-preference>
</portlet-definition>
